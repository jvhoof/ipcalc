{
  "name": "cloud-network-diagram",
  "version": "0.1.0",
  "description": "Vue.js component library for creating cloud network diagrams with draw.io export",
  "type": "module",
  "main": "./dist/cloud-network-diagram.umd.cjs",
  "module": "./dist/cloud-network-diagram.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/cloud-network-diagram.js",
      "require": "./dist/cloud-network-diagram.umd.cjs",
      "types": "./dist/index.d.ts"
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit"
  },
  "dependencies": {
    "vue": "^3.4.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@vitejs/plugin-vue": "^4.5.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.6.0",
    "vue-tsc": "^1.8.0"
  },
  "peerDependencies": {
    "vue": "^3.4.0"
  },
  "keywords": [
    "vue",
    "network-diagram",
    "cloud",
    "azure",
    "aws",
    "gcp",
    "draw.io",
    "diagram"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jvhoof/ipcalc"
  }
}
